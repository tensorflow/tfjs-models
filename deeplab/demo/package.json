{
    "name": "deeplab-demo",
    "version": "0.0.1",
    "description": "",
    "main": "src/index.js",
    "license": "Apache-2.0",
    "private": true,
    "engines": {
        "node": ">=8.9.0"
    },
    "peerDependencies": {
        "@tensorflow-models/deeplab": "^0.1.0"
    },
    "dependencies": {
        "@tensorflow/tfjs": "^1.1.0",
        "bulma": "^0.7.5",
        "stats.js": "^0.17.0"
    },
    "scripts": {
        "watch": "cross-env NODE_ENV=development parcel src/index.html --no-hmr --open ",
